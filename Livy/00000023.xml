                    </position>
                </language>
                <translation>authors</translation>
            </marginalia>
            <marginalia hand="Italian">
                <language ident="EL">
                    <position place="tail" book_orientation="0">
                        <marginalia_text>διότι</marginalia_text>
                    </position>
                </language>
                <language ident="LA">
                    <position place="tail" book_orientation="0">
                        <marginalia_text>politicum, et polemicum.</marginalia_text>
                    </position>
